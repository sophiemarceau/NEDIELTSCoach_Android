<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android_custom="http://schemas.android.com/apk/res/com.example.hello"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
<View 
    android:layout_width="match_parent"
    android:layout_height="10dp"
    android:background="@color/describe_color"
    /> 
    
<RelativeLayout 
    android:layout_width="match_parent"
    android:layout_height="100dp"
    >

    <TextView
        android:id="@+id/item_txtname_test_report_complet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="13dp"
        android:textSize="16sp"
        android:text="1.填空题名称" />

    <TextView
        android:id="@+id/item_txtanswer_test_report_complet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/item_txtname_test_report_complet"
        android:layout_below="@+id/item_txtname_test_report_complet"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="13dp"
        android:textColor="@color/font_color"
        android:singleLine="false"
        android:maxEms="12"
        android:text="正确答案：apple" />

    <com.lelts.tool.RoundProgressBar
        android:id="@+id/roundProgressBar3_complet"
        android:layout_width="60dip"
        android:layout_height="60dip"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/item_txtname_test_report_complet"
        android:layout_marginRight="42dp"
        android_custom:roundColor="#e84d60"
        android_custom:roundProgressColor="#e84d60"
        android_custom:roundWidth="6dip"
        android_custom:textIsDisplayable="false" />

    <com.lelts.tool.RoundProgressBar
        android:id="@+id/roundProgressBar2_complet"
        android:layout_width="60dip"
        android:layout_height="60dip"
        android:layout_alignBottom="@+id/roundProgressBar3_complet"
        android:layout_alignLeft="@+id/roundProgressBar3_complet"
        android_custom:roundColor="#D1D1D1"
        android_custom:roundProgressColor="#e84d60"
        android_custom:roundWidth="6dip"
        android_custom:textColor="#e84d60"
        android_custom:textSize="18sp" />

    <TextView
        android:id="@+id/item_txttrue_test_report_complet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/item_txtanswer_test_report_complet"
        android:layout_alignParentRight="true"
        android:layout_marginRight="26dp"
        android:background="@drawable/zhengquelv"
        android:textColor="@color/white"
        android:gravity="center"
        android:text="正确率" />
    
</RelativeLayout>
<View 
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:background="@color/describe_color"
    />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="157dp" >



    <TextView
        android:id="@+id/btn1_complete_default"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="@dimen/refer_margin"
        android:layout_marginRight="@dimen/refer_margin"
        android:layout_marginTop="18dp"
        android:background="@drawable/edit_round_defalut"
        android:gravity="center_vertical"
        />
        <TextView
        android:id="@+id/btn1_complet"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="18dp"
        android:background="@drawable/edit_round"
        android:gravity="center_vertical"
        android:textColor="#FFFFFF" />

   

    <TextView
        android:id="@+id/txt_defalut_complet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/btn1_complete_default"
        android:layout_alignBottom="@+id/btn1_complete_default"
        android:layout_alignLeft="@+id/btn1_complete_default"
        android:textColor="@color/white"
        android:text="正确答案" />

    <TextView
        android:id="@+id/btn2_complete_default"
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/btn1_complete_default"
        android:layout_alignRight="@+id/btn1_complete_default"
        android:layout_below="@+id/btn1_complete_default"
        android:layout_marginTop="31dp"
        android:background="@drawable/edit_round_defalut"
        android:textColor="@color/white"
        android:text="other"
        android:gravity="center_vertical" />

     <TextView
         android:id="@+id/btn2_complet"
         android:layout_width="wrap_content"
         android:layout_height="30dp"
         android:layout_alignBaseline="@+id/btn2_complete_default"
         android:layout_alignBottom="@+id/btn2_complete_default"
         android:layout_alignLeft="@+id/btn2_complete_default"
         android:background="@drawable/edit_round2"
         android:gravity="center_vertical"
         android:text="other"
         android:textColor="#FFFFFF" />

    </RelativeLayout>

</LinearLayout>
